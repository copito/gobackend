{
    "cSpell.words": [
        "gorm",
        "idempotently",
        "Outbounds"
    ],
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "database": "${workspaceFolder:data_quality}/services/backend/src/test.db",
            "name": "test data"
        }
    ],
    "launch": {
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Golang Launch Package",
                "type": "go",
                "request": "launch",
                "mode": "auto",
                "program": "${workspaceFolder}/services/backend/src/main.go",
                "console": "integratedTerminal"
            }

        ],
        "compounds": []
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "bazel-*": true,
        "pkg*": true
    },
    "explorerExclude.backup": {},

}